{
  "name": "jinaai",
  "version": "0.2.0",
  "description": "Jina AI JavaScript SDK",
  "main": "dist/src/jinaai.js",
  "types": "dist/src/jinaai.d.ts",
  "engines": {
    "node": "^18"
  },
  "scripts": {
    "build": "tsc",
    "test": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --logHeapUsage --runInBand --config ./jest.base.js --coverage --forceExit",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jina-ai/jinaai-js.git"
  },
  "keywords": [
    "Jina AI",
    "Jina"
  ],
  "contributors": [
    "Guillaume Roncari <guillaume.roncari@jina.ai>"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jina-ai/jinaai-js/issues"
  },
  "homepage": "https://github.com/jina-ai/jinaai-js#readme",
  "dependencies": {
    "tslib": "^2.5.3",
    "undici": "^5.19.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "husky": "^8.0.1",
    "jest": "^29.5.0",
    "jest-summary-reporter": "^0.0.2",
    "lint-staged": "^11.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint"
    ]
  },
  "files": [
    "dist/src"
  ]
}